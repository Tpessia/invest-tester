{
  "name": "invest-tester",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "init": "cd ../ && git clone https://github.com/Tpessia/dados-financeiros",
    "start": "vite --mode ${ENV:-dev} --host 0.0.0.0 --port 3001",
    "build": "tsc && vite build --mode ${ENV:-prod}",
    "build:view": "npx vite-bundle-visualizer -c vite.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docker:base": "sudo docker compose -f ./docker-compose.yml --env-file ./deploy/.env.docker.${ENV:-dev}",
    "docker:config": "npm run docker:base -- config",
    "docker:build": "npm run docker:base -- build --parallel",
    "docker:run": "npm run docker:base -- up",
    "docker:logs": "npm run docker:base -- logs -f",
    "docker:rm": "npm run docker:base -- rm --force",
    "docker:exec": "npm run docker:base -- exec invest-tester /bin/bash",
    "docker:sast": "sudo docker run --rm -v \"${PWD}:/src\" returntocorp/semgrep semgrep --metrics=off --no-git-ignore --config \"/src/.semgrep.yml\" --config \"p/secrets\" --config \"p/gitleaks\" --config \"p/owasp-top-ten\" --config \"p/cwe-top-25\" --config \"p/ci\" --config \"p/security-audit\" --config \"p/javascript\" --config \"p/typescript\" --config \"p/docker\" --config \"p/docker-compose\" --config \"p/nginx\" --config \"p/react\" /src # --verbose"
  },
  "dependencies": {
    "@ant-design/icons": "5.3.7",
    "@nivo/core": "^0.87.0",
    "@nivo/line": "^0.87.0",
    "antd": "5.17.4",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "color": "4.2.3",
    "dayjs": "1.11.11",
    "decimal.js": "10.4.3",
    "event-lite": "0.1.3",
    "immutability-helper": "3.1.1",
    "lodash-es": "4.17.21",
    "marked": "^12.0.2",
    "react": "18.2.0",
    "react-ace": "11.0.1",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-number-format": "5.4.0",
    "react-router-dom": "6.23.1",
    "react-transition-group": "4.4.5",
    "reflect-metadata": "0.2.2",
    "tsyringe": "4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/color": "3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "jsdom": "^24.1.0",
    "sass": "1.77.2",
    "ts-mockito": "^2.6.1",
    "typescript": "5.2.2",
    "vite": "5.2.12",
    "vitest": "1.6.0"
  }
}
